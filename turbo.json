{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:prod": {
      "cache": true,
      "dependsOn": ["^build:prod"],
      "outputs": ["dist/**"]
    },
    "build:w": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "persistent": true
    },
    "test": {
      "cache": true,
      "dependsOn": ["^build"]
    },
    "test:w": {
      "cache": false,
      "dependsOn": ["^build"],
      "persistent": true
    },
    "coverage": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "dev": {}
  }
}
