{
  "name": "@modular-rocks/slimfast-turbo",
  "description": "Slimfast super-repo",
  "private": true,
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "build": "turbo run build",
    "build:w": "turbo run build:w",
    "build:prod": "turbo run build:prod",
